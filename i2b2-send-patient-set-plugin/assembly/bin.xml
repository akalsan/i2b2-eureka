<assembly>
	<id>bin</id>
	<formats>
		<format>zip</format>
	</formats>
	<fileSets>
		<fileSet>
			<directory></directory>
			<outputDirectory></outputDirectory>
			<includes>
				<include>README*</include>
			</includes>
			<filtered>true</filtered>
		</fileSet>	
		<fileSet>
			<directory></directory>
			<outputDirectory></outputDirectory>
			<includes>
				<include>LICENSE*</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/webapp/js</directory>
			<outputDirectory>PatientSetSender</outputDirectory>
			<includes>
				<include>*.js</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/webapp/images</directory>
			<outputDirectory>PatientSetSender/assets/images</outputDirectory>
			<includes>
				<include>*</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/webapp/html</directory>
			<outputDirectory>PatientSetSender/assets</outputDirectory>
			<includes>
				<include>injected_screens.html</include>
				<include>patient_set_sender.html</include>
				<include>prototype-1.7.3.js</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/webapp/css</directory>
			<outputDirectory>PatientSetSender/assets</outputDirectory>
			<includes>
				<include>PatientSetSender.css</include>
			</includes>
		</fileSet>
	</fileSets>
</assembly>
